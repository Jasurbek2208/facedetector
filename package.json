{
  "name": "facedetector",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "vite --open /dist/index.html",
    "dev": "vite",
    "host": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "deploy": "scp -r dist package.json root@SERVER_IP_ADRESS:/var/www/facedetector.uz/",
    "preview": "vite preview"
  },
  "author": {
    "name": "Jasurbek Shomaqsudov",
    "email": "jasurbekshomaqsudov2004@gmail.com",
    "url": "https://shomaqsudov.uz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jasurbek2208/facedetector"
  },
  "license": "MIT",
  "homepage": "https://facedetector.shomaqsudov.uz",
  "bugs": {
    "url": "https://github.com/Jasurbek2208/facedetector/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 edge version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "jsxSingleQuote": true,
    "arrowParens": "always",
    "printWidth": 200,
    "tabWidth": 2,
    "useTabs": false,
    "endOfLine": "auto",
    "bracketSameLine": true
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "eslintIgnore": [
    "vite.config.ts"
  ],
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "face-api.js": "^0.22.2",
    "million": "^3.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.8",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
